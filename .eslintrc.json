{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "import/no-dynamic-require": "off",
        "global-require": "off",
        "no-console": "off",
        "no-plusplus": "off",
        "no-underscore-dangle": "off",
        "arrow-parens": "off"
    }
}